{
  "author": "Vitalii Maslianok <maslianok@gmail.com> (https://github.com/maslianok)",
  "version": "0.5.4",
  "bugs": {
    "url": "https://github.com/maslianok/react-responsive-tabs/issues"
  },
  "description": "React responsive tabs",
  "peerDependencies": {
    "react": "^0.14.7 || ^15.1.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint-config-airbnb",
    "env": {
      "browser": true,
      "mocha": true
    },
    "plugins": [
      "react",
      "import"
    ],
    "rules": {
      "no-underscore-dangle": 0,
      "react/jsx-filename-extension": 0,
      "jsx-a11y/no-static-element-interactions": 0
    },
    "settings": {
      "import/resolve": {
        "moduleDirectory": ["node_modules"]
      }
    }
  },
  "dependencies": {
    "js-stylesheet": "0.0.1",
    "react-resize-detector": "^0.3.0",
    "classnames": "^2.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.6.0",
    "react": "^15.1.0",
    "rimraf": "^2.3.4",
    "webpack": "^1.12.14"
  },
  "directories": {
    "example": "example"
  },
  "homepage": "https://github.com/maslianok/react-responsive-tabs",
  "keywords": [
    "react",
    "responsive",
    "tabs",
    "tab"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "maintainers": [
    "maslianok <maslianok@gmail.com>"
  ],
  "name": "react-responsive-tabs",
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maslianok/react-responsive-tabs.git"
  },
  "scripts": {
    "build": "babel --presets=react,es2015 --plugins=transform-object-assign src --out-dir lib",
    "clean": "rimraf lib",
    "lint": "eslint --ignore-path .gitignore -- src",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel/register --recursive --watch"
  }
}
